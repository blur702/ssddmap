  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:ci": "playwright test --reporter=html",
    "test:headed": "playwright test --headed",
    "lint": "eslint public/js/**/*.js services/**/*.js server.js --fix-dry-run",
    "lint:fix": "eslint public/js/**/*.js services/**/*.js server.js --fix",
    "lint:check": "eslint public/js/**/*.js services/**/*.js server.js",
    "lint:staged": "eslint --fix",
    "audit": "npm audit --audit-level moderate",
    "health": "curl -f http://localhost:3001 || exit 1",
    "pretest": "npm run health",
    "hooks:install": "./install-hooks.sh",
    "hooks:uninstall": "./uninstall-hooks.sh",
    "hooks:test": "echo 'Testing hooks...' && npm run lint && npm run health",
    "commit:safe": "git add -A && git commit",
    "commit:force": "git add -A && git commit --no-verify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
